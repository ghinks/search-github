{
  "name": "@gvhinks/search-github",
  "version": "1.0.0",
  "description": "A package that provides an API and a CLI to query the github graph interface for  open issues matching search term(s). This does require a github token see authentication section.",
  "bin": {
    "search-github": "bin/index.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js",
    "watch": "babel --watch src --out-dir lib --ignore test.js",
    "lint": "standard --env jest src/**/*.js",
    "test": "jest --coverage"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghinks/search-github.git"
  },
  "author": "Glenn Hinks",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-runtime": "6.26.0",
    "graphql-request": "^1.5.1",
    "lodash.get": "^4.4.2",
    "minimist": "^1.2.0",
    "moment": "^2.22.1",
    "ora": "^2.1.0",
    "parse-git-config": "^2.0.2",
    "table": "^4.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^22.4.3",
    "nock": "^9.2.5",
    "regenerator-runtime": "^0.11.1",
    "standard": "^11.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "jest": true
    }
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "10.0.0"
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-rewire",
          "transform-runtime"
        ]
      }
    }
  },
  "bugs": {
    "url": "https://github.com/ghinks/search-github/issues"
  },
  "homepage": "https://github.com/ghinks/search-github#readme",
  "directories": {
    "lib": "lib"
  }
}
